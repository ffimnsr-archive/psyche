{
  "name": "psyche",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "yarn run watch",
    "build": "webpack --config webpack.development.babel.js --mode development --no-color",
    "build.production": "webpack --config webpack.production.babel.js --mode production --no-color",
    "build.types": "tsc --noEmit false --emitDeclarationOnly",
    "watch": "webpack serve --config webpack.development.babel.js --no-color",
    "lint": "eslint */**/*.{ts,tsx}",
    "lint.fix": "eslint */**/*.{ts,tsx} --quiet --fix",
    "lint.full": "tsc --noEmit && eslint */**/*.{ts,tsx} --quiet --fix",
    "test": "mocha",
    "postinstall": "husky install",
    "mockql": "json-graphql-server ./data/mock-db.js",
    "schema.manager": "apollo client:download-schema -c ./apollo/apollo.manager.config.js ./apollo/generated_schemas/schema.manager.graphql",
    "schema.member": "apollo client:download-schema -c ./apollo/apollo.member.config.js ./apollo/generated_schemas/schema.member.graphql",
    "schema.anonymous": "apollo client:download-schema -c ./apollo/apollo.anonymous.config.js ./apollo/generated_schemas/schema.anonymous.graphql"
  },
